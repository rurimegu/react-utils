{
  "name": "@rurino/react-utils",
  "version": "0.0.4",
  "description": "React utilities for the @rurino project.",
  "type": "module",
  "author": "KCFindstr",
  "license": "MIT",
  "keywords": [
    "lyrics",
    "megumi",
    "rurino",
    "utilities",
    "react"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --config vite-web.config.ts",
    "build": "vite build",
    "watch": "vite build --watch",
    "gen:lock": "npm i --package-lock-only --workspaces false",
    "lint": "eslint ."
  },
  "dependencies": {
    "@rurino/core": "0.0.5",
    "@uidotdev/usehooks": "^2.4.1",
    "clsx": "^2.1.1",
    "d3-ease": "^3.0.1",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-circular-progressbar": "^2.1.0",
    "react-dom": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@modyfi/vite-plugin-yaml": "^1.1.0",
    "@monaco-editor/react": "^4.6.0",
    "@types/d3-ease": "^3.0.2",
    "@types/lodash": "^4.17.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "postcss": "^8.4.44",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.4.0",
    "vite": "^5.4.1",
    "vite-plugin-dts": "^4.1.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bugs": {
    "url": "https://github.com/rurimegu/react-utils/issues"
  },
  "homepage": "https://github.com/rurimegu/react-utils#readme"
